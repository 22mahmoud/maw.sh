{
  "version": 1,
  "formatters": {
    "json_request": {
      "()": "src.base.logging.JsonRequestFormatter"
    },
    "json_error": {
      "()": "src.base.logging.JsonErrorFormatter"
    }
  },
  "handlers": {
    "json_request": {
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout",
      "formatter": "json_request"
    },
    "json_error": {
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout",
      "formatter": "json_error"
    }
  },
  "root": {
    "level": "INFO",
    "handlers": []
  },
  "loggers": {
    "gunicorn.access": {
      "level": "INFO",
      "handlers": ["json_request"],
      "propagate": false
    },
    "gunicorn.error": {
      "level": "INFO",
      "handlers": ["json_error"],
      "propagate": false
    }
  }
}
